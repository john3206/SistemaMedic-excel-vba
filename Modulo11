Public Sub RestaurarExcel()
    On Error Resume Next
    With Application
        .DisplayFormulaBar = True
        .DisplayStatusBar = True
        .ExecuteExcel4Macro "SHOW.TOOLBAR(""Ribbon"", True)"
    End With

    With ActiveWindow
        .DisplayGridlines = True
        .DisplayHeadings = True
        .DisplayWorkbookTabs = True
    End With
End Sub

